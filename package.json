{
  "name": "@workadventure/quests",
  "version": "1.1.6",
  "description": "Utility functions to create quests for WorkAdventure",
  "type": "module",
  "main": "./dist/quests.umd.cjs",
  "module": "./dist/quests.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "author": "David NÃ©grier <david@workadventu.re>",
  "exports": {
    ".": {
      "import": "./dist/quests.js",
      "require": "./dist/quests.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview --host",
    "test": "vitest",
    "lint": "eslint src/ --ext .ts",
    "fix": "eslint src/ --ext .ts --fix",
    "semantic-release": "semantic-release",
    "semantic-release-dry-run": "semantic-release -d",
    "pretty": "prettier --write src/**/*.ts"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^10.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^10.0.4",
    "@types/node": "^20.4.8",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@workadventure/iframe-api-typings": "^1.16.5",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "3.0.1",
    "semantic-release": "^21.0.7",
    "typescript": "^5.1.6",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.5.1",
    "vitest": "^0.34.1"
  },
  "lint-staged": {
    "*.ts": [
      "npm run fix",
      "npm run pretty"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/workadventure/quests.git"
  }
}
